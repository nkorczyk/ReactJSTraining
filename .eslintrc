{
  "parser": "babel-eslint",
  "rules": {
      "max-len": [1, 120, 2, {"ignoreComments": true }],
      "prop-types": [2],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "extends": ["airbnb-base"],
  "plugins": [
      "import",
      "react",
      "jsx-a11y"
  ]
}
