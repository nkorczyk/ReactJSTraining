{
  "name": "reactjsmentoringprogram2018",
  "version": "1.0.0",
  "description": "ReactJS Mentoring Program",
  "entry point": "srcServer.js",
  "main": "script.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development  node ./buildScripts",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack",
    "prod": "cross-env NODE_ENV=production  node ./buildScripts",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore --fix",
    "test": "set NODE_ENV=test&&jest --verbose",
    "test:watch": "npm test --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:e2e": "cypress open"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.jsx",
      "!src/actions/store.js",
      "!src/reducers/index.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkorczyk/ReactJSTraining.git"
  },
  "author": "Norbert Korczyk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nkorczyk/ReactJSTraining/issues"
  },
  "homepage": "https://github.com/nkorczyk/ReactJSTraining#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "cross-env": "^5.1.6",
    "express": "^4.16.4",
    "materialize-css": "^1.0.0-rc.2",
    "react": "16.3.2",
    "react-dom": "16.3.3",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^3.6.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "0.28.11",
    "cypress": "^3.1.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "expect": "^23.6.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fetch-mock": "^7.2.5",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "open": "0.0.5",
    "react-test-renderer": "^16.6.3",
    "redux-mock-store": "^1.5.3",
    "style-loader": "0.20.3",
    "url-loader": "^1.1.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
